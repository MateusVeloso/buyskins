        <header>
            <% if (!user) { %>
            <a href="auth/steam" class="steambutton">
                <span>Entrar com Steam</span>
                <div class="icon">
                    <i class="fa fa-steam-square"></i>
                </div>
            </a>
            <!--<h2>Welcome! Please log in.</h2>-->
            <!--<p><a href="auth/steam">Sign On with Steam</a></p>-->
            <% } else  { %>
            <a class="profile" href="#">
                <span>BC : 0,00</span>
                <img src='<%= user.photos[2].value %>' alt='Avatar' class="usu-avatar"/>
            </a>
            <div class="container" id="profile-background">
                <div class="seta"></div>
                <div class="row">
                    <div class="col-md-12 profile-body">
                        <div class="row">
                            <img src="http://oi64.tinypic.com/10pdrvo.jpg" class="selected-medal">
                            <div class="col-xs-2 profile-avatar"><img src="<%= user.photos[2].value %>" class="avatar-image"></div>
                            <div class="col-xs-8 profile-name"><%= user.displayName %></div>

                            <div class="col-xs-2 commendations">
                                <div class="friendly"><img src="http://oi68.tinypic.com/259wiv4.jpg"><span>0</span></div>
                                <div class="teacher"><img src="http://oi68.tinypic.com/2iwa9hg.jpg"><span>0</span></div>
                                <div class="leader"><img src="http://oi66.tinypic.com/k0mrgw.jpg"><span>0</span></div>
                            </div>

                            <div class="col-xs-8 medals">
                                <img src="http://oi64.tinypic.com/10pdrvo.jpg">
                                <img src="http://oi65.tinypic.com/sfeykl.jpg">
                                <img src="http://oi68.tinypic.com/23ktdi1.jpg">
                                <img src="http://oi65.tinypic.com/2i70mdl.jpg">
                                <img src="http://oi64.tinypic.com/1zcfmn5.jpg">
                                <img src="http://oi63.tinypic.com/254x36h.jpg">
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-xs-2"><img src="http://oi63.tinypic.com/2ufehvn.jpg" class="rank-image"></div>

                            <div class="col-xs-6 current-rank"><b>Master Sergeant</b> Rank 16</div>
                            <div class="col-xs-4 next-rank">Rank 17 + Drop</div>
                            <div class="col-xs-12 xp-progress-bar">
                                <div class="xp-earned"></div>
                            </div>
                        </div>

                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12 profile-footer">
                        <div class="col-xs-2"><img src="http://oi65.tinypic.com/xlf57p.jpg" class="skill-group-image"></div>

                        <div class="col-xs-8 skill-group-name">Global Elite</div>
                        <div class="col-xs-1 matches-won"><img src="http://oi65.tinypic.com/i44dgk.jpg" alt="Quantidade de vendas"><span>1337</span></div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12 profile-footer">
                        <a href="logout" class="steambutton steambutton-sair">
                            <span>Sair</span>
                            <div class="icon">
                                <i class="fa fa-steam-square"></i>
                            </div>
                        </a>
                        <a href="iventario" class="steambutton steambutton-iventario">
                            <span>Ivent√°rio</span>
                            <div class="icon">
                                <i class="fa fa-suitcase"></i>
                            </div>
                        </a>
                    </div>
                </div>
            </div>
            <!--<h2>Hello, <%= user.displayName %>. - <a href='logout'>Logout</a></h2>-->
            <% } %>

